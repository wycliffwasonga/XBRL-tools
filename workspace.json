{
  "version": 2,
  "projects": {
    "xbrl-viewer": "apps/xbrl-viewer",
    "xbrl-viewer-e2e": "apps/xbrl-viewer-e2e"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "standaloneConfig": true
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "standaloneConfig": true,
      "addModuleSpec": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "xbrl-viewer"
}
